# do not edit this file, it will be overwritten on update
# To manually activate:
#    sudo cp 77-mm-adelino-greylist.rules /lib/udev/rules.d
#    sudo udevadm control --reload
#    sudo udevadm trigger

ACTION!="add|change|move", GOTO="mm_adelino_greylist_end"
SUBSYSTEM!="usb", GOTO="mm_adelino_greylist_end"
ENV{DEVTYPE}!="usb_device",  GOTO="mm_adelino_greylist_end"

# Devices using Microchip's assigned VID/PID
# Adelino M1
ATTRS{idVendor}=="04d8", ATTRS{idProduct}=="eddf", ENV{ID_MM_DEVICE_MANUAL_SCAN_ONLY}="1"

LABEL="mm_adelino_greylist_end"

